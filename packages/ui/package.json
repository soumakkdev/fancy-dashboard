{
	"name": "ui",
	"version": "1.0.0",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"license": "MIT",
	"exports": {
		".": "./dist",
		"./styles.css": "./dist/styles.css"
	},
	"scripts": {
		"build": "tsup && tailwindcss -i ./src/styles.css -o ./dist/styles.css",
		"dev": "concurrently \"tsup --watch\" \"tailwindcss -i ./src/styles.css -o ./dist/styles.css --watch\"",
		"clean": "rimraf dist"
	},
	"devDependencies": {
		"@types/react": "^18.0.26",
		"@types/react-dom": "^18.0.9",
		"concurrently": "^7.2.2",
		"eslint": "^7.32.0",
		"eslint-config-custom": "*",
		"react": "^18.2.0",
		"rimraf": "^3.0.2",
		"tailwind-config": "*",
		"tailwindcss": "^3.2.4",
		"tsconfig": "*",
		"tsup": "^6.1.3",
		"typescript": "^4.9.4"
	}
}
